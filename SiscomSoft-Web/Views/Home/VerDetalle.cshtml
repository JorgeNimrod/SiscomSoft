@using SiscomSoft.Models;
@{
    ViewBag.Title = "VerDetalle";
    SiscomSoft.Models.Categoria id = ViewBag.id;
}

<form method="post">
    <div class="col-md-12">
        <div class="form-group ">
            <label for="Nombre">Categoria</label>
            <input type="text" class="form-control" id="Nombre" name="Nombre" value='@id.sNombre' readonly="readonly">
        </div>
        <div class="form-group ">
            <label for="Apellido">Sub Categoria</label>
            <input type="text" class="form-control " id="Apellido" name="Apellido" value='@id.sNomSubCat' readonly="readonly">
        </div>
    </div>
</form>

